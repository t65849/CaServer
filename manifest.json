{
    "name": "Extension測試",
    "version": "1.0",
    "description": "A plain text description",
    "permissions": ["storage", "activeTab","http://localhost/*","http://127.0.0.1/*","http://127.0.0.1:1337/*","http://127.0.0.1/*"],
    "background": {
        "scripts": ["background.js","/pages/scripts/jquery/jquery-1.9.0.min.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/images16.png",
            "32": "images/images32.png",
            "48": "images/images48.png",
            "128": "images/images128.png"
        }
    },
    "icons": {
        "16": "images/images16.png",
        "32": "images/images32.png",
        "48": "images/images48.png",
        "128": "images/images128.png"
    },
    "options_page": "options.html",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": [
            "http://*/*",
            "https://*/*"
          ],
          "js": [
            "/pages/scripts/jquery/jquery-1.9.0.min.js",
            "event.js"
          ],
          "css": ["/pages/css/air.css"],
          "run_at": "document_end"
        }
      ]


}